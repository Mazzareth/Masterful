{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/masterwork/browser"
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(assets|favicon.ico|manifest.webmanifest)/(.*)",
      "dest": "/dist/masterwork/browser/$1/$2"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/masterwork/browser/index.html"
    }
  ]
}