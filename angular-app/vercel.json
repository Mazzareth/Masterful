{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "dist/angular-app/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(assets|favicon.ico|manifest.webmanifest)/(.*)",
      "dest": "/dist/angular-app/browser/$1/$2"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/angular-app/browser/index.html"
    }
  ]
}